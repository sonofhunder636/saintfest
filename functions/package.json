{
  "name": "saintfest-functions",
  "version": "1.0.0",
  "description": "Firebase Functions for SaintFest Next.js Application",
  "main": "index.js",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start --only functions",
